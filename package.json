{
  "name": "framer-dynamic-graph",
  "version": "1.0.0",
  "description": "Dynamic graph component for Framer that fetches data from Google Sheets and creates intelligent visualizations",
  "main": "components/DynamicGraph.tsx",
  "scripts": {
    "build": "tsc --noEmit",
    "check": "tsc --noEmit",
    "dev": "tsc --noEmit --watch",
    "test": "jest --run",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --run",
    "test:unit": "jest --testPathPattern=tests/.*\\.test\\.(ts|tsx)$",
    "test:integration": "jest --testPathPattern=tests/.*\\.integration\\.test\\.(ts|tsx)$",
    "test:performance": "jest --testPathPattern=tests/.*\\.performance\\.test\\.(ts|tsx)$",
    "test:framer": "jest --testPathPattern=tests/.*\\.framer\\.test\\.(ts|tsx)$"
  },
  "keywords": [
    "framer",
    "react",
    "data-visualization",
    "google-sheets",
    "charts",
    "dashboard",
    "components"
  ],
  "author": "Dynamic Graph Team",
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "framer-motion": "^12.23.12",
    "openai": "^5.13.1",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "recharts": "^3.1.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "babel-jest": "^29.7.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  }
}
